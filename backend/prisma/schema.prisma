generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Models Riot Games
model WhatsHappening {
  id        Int      @id @default(autoincrement())
  title     String
  icon      String
  image     String
  link      String
  thumbMain Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
